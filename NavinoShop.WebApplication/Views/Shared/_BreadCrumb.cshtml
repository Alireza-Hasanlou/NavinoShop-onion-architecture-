@using Shared.Ui
@model List<BreadCrumb>
<div class="container-fluid" id="page-hero">
    <div class="row">
        <div class="col-12">
            <div class="container">
                <div class="row">
                    <div class="col-12 px-0">
                        <nav aria-label="breadcrumb">
                            <ol class="breadcrumb">
                                @foreach (var item in Model)
                                {
                                    <li><a href="@item.Url"> @item.Title </a></li>
                                }
                            </ol>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
