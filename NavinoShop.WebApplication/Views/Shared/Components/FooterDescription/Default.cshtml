@model Site.Application.Contract.SiteSettingService.Query.FooterUiQueryModel


<div class="col-12 col-md-12 col-lg-6">

    @if (!string.IsNullOrEmpty(Model.LogoName))
    {
        <hr class="d-lg-none">
        <img src="@Model.LogoName" alt="@((!string.IsNullOrEmpty(Model.LogoAlt)) ? Model.LogoAlt : "")">
        <span>روبیک مارکت</span>

    }
    @if (!string.IsNullOrEmpty(Model.Description))
    {
        <p>@Model.Description</p>
    }
    <div class="row">
        <div class="col-12 col-md-6 text-center p-2" id="encode43vfbb65gtabfb">
            <div>7 روز هفته، 24 ساعت شبانه روز</div>
            <div>پاسخگوی شما هستیم</div>
            <div>@Model.Phone1</div>
            <div>@Model.Email1</div>
           
        </div>
        <div class="col-12 col-md-12 pt-2 pt-md-0" id="encode43vfbbbfb">
            <div class="row">
                <div class="col-6 text-center">
                    @if (string.IsNullOrEmpty(Model.Enamad))
                    {
                        <a href="#"><img src="@Model.Enamad" alt=""></a>
                    }

                </div>
                <div class="col-6 text-center">
                    @if (string.IsNullOrEmpty(Model.SamanDehi))
                    {
                        <a href="#"><img src="@Model.SamanDehi" alt=""></a>
                    }

                </div>

            </div>
        </div>
    </div>
</div>
<section class="py-2" id="copyright">
    <div class="container">
        <div class="row">
            <div class="col-12 col-sm-6 " id="social-links">
                <span>ما را دنبال کنید</span>

                <a href="@(!string.IsNullOrEmpty(Model.InstagrmLink)?Model.InstagrmLink:"#")"><img src="/images/social/insta.png" alt=""></a>
                <a href="@(!string.IsNullOrEmpty(Model.YoutubeLink) ? Model.YoutubeLink : "#")"><img src="/images/social/youtube.png" alt=""></a>
                <a href="@(!string.IsNullOrEmpty(Model.TelegramLink) ? Model.TelegramLink : "#")"><img src="/images/social/Telegram.16.png" alt=""></a>
                <a href="@(!string.IsNullOrEmpty(Model.WhatsupLink) ? Model.WhatsupLink : "#")"><img src="/images/social/Whatsapp.16.png" alt=""></a>
            </div>
            <div class="col-12 col-sm-6 text-sm-end pt-2 pt-sm-0">
                <span>کلیه حقوق و مادی معنوی محفوط است.</span>
            </div>
        </div>
    </div>
</section>