@page
@model NavinoShop.WebApplication.Areas.Admin.Pages.Post.City.EditModel

@{
    Layout = "_AdminLayout";
    ViewData["title"] = "ویرایش استان  ";

}
<div class="Formcontainer">

    <form id="Formcontainer" method="post" enctype="multipart/form-data">

        <input type="hidden" asp-for="Editcity.Id" />
        <input type="hidden" asp-for="Editcity.StateId" />
        <div class="col col-12 col-sm-12 ">

            <div class="custom-form-group">
                <labkel for="title" class="required">نام شهر</labkel>
                <input asp-for="@Model.Editcity.Title" type="text" id="Title">
                <span asp-validation-for="Editcity.Title" class="text-danger"></span>
            </div>
        </div>

        <button type="submit" class="btn btn-success btn-block">ویرایش </button>

    </form>
</div>
